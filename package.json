{
  "name": "vue-nuxt",
  "version": "1.0.0",
  "description": "Vue + Nuxt",
  "author": "Stephen Lombard <stephen@rainmaker.travel>",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "1338"
    }
  },
  "scripts": {
    "dev": "nuxt --config-file nuxt.config.js",
    "build": "nuxt build --config-file nuxt.config.js",
    "start": "nuxt start --config-file nuxt.config.js",
    "generate": "nuxt generate --config-file nuxt.config.js",
    "postgenerate": "node ./minifier",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/vue-fontawesome": "0.0.23",
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/dotenv": "^1.1.1",
    "@nuxtjs/pwa": "^2.0.8",
    "@nuxtjs/vuetify": "^0.4.1",
    "@rainmakerdigital/nuxt-vista-sdk": "^1.0.1",
    "@rainmakerdigital/vista-sdk": "^1.0.11",
    "ajv": "^5.0.0",
    "ajv-keywords": "^2.1.1",
    "async": "^2.6.1",
    "clone": "^1.0.4",
    "core-js": "^2.5.5",
    "debug": "^3.1.0",
    "firebase": "^5.5.1",
    "fs-extra": "^6.0.1",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.0",
    "node-sass-vendor": "^4.8.3",
    "nuxt-fontawesome": "^0.2.0",
    "nuxt-material-design-icons": "^1.0.4",
    "sass-loader": "^7.0.2",
    "store": "^2.0.12",
    "vue": "^2.5.16",
    "vue-facebook-page": "^1.0.1",
    "vue-fontawesome": "0.0.2",
    "vue-meta": "^1.5.0",
    "vue-no-ssr": "^1.0.0",
    "vue-router": "^3.0.1",
    "vue-warehouse": "^1.0.0",
    "vue2-google-maps": "^0.10.2",
    "webfontloader": "^1.6.28",
    "webpack": "^3.12.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0"
  }
}
